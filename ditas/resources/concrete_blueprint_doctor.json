        "paths": {
            "/patient/{SSN}": {
                "get": {
                    "summary": "Get patient's biographical data",
                    "description": "This method returns the biographical data for the specified patient (identified via SSN), to be used by medical doctors",
                    "operationId": "getPatientBiographicalData",
                    "parameters": [{
                        "name": "SSN",
                        "in": "path",
                        "description": "SSN of the patient",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }],
                    "responses": {
                        "200": {
                            "description": "Biographical data of the patient",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "SSN": {
                                                "type": "string"
                                            },
                                            "name": {
                                                "type": "string"
                                            },
                                            "surname": {
                                                "type": "string"
                                            },
                                            "gender": {
                                                "type": "string"
                                            },
                                            "birthDate": {
                                                "type": "string"
                                            },
                                            "addressCity": {
                                                "type": "string"
                                            },
                                            "addressRoad": {
                                                "type": "string"
                                            },
                                            "addressRoadNumber": {
                                                "type": "integer"
                                            },
                                            "addressPostalCode": {
                                                "type": "integer"
                                            },
                                            "telephoneNumber": {
                                                "type": "integer"
                                            },
                                            "birthCity": {
                                                "type": "string"
                                            },
                                            "nationality": {
                                                "type": "string"
                                            },
                                            "job": {
                                                "type": "string"
                                            },
                                            "schoolYears": {
                                                "type": "integer"
                                            },
                                            "familyDoctorId": {
                                                "type": "string"
                                            },
                                            "_links": {
                                                "type": "object",
                                                "properties": {
                                                    "self": {
                                                        "type": "object",
                                                        "properties": {
                                                            "href": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "required": ["href"]
                                                    },
                                                    "blood-test-summary": {
                                                        "type": "object",
                                                        "properties": {
                                                            "href": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "required": ["href"]
                                                    }
                                                },
                                                "required": ["self", "blood-test-summary"]
                                            }
                                        },
                                        "required": ["patientId", "SSN", "name", "surname", "gender", "birthDate", "addressCity", "addressRoad", "addressRoadNumber", "addressPostalCode", "telephoneNumber", "birthCity", "nationality", "job", "schoolYears", "familyDoctorId", "_links"]
                                    }
                                }
                            }
                        },
                        "404": {
                            "description": "Patient not found"
                        }
                    },
                    "x-data_sources": ["patientBiographicalData"]
                }
            },
            "/patient/{SSN}/blood-test/summary": {
                "get": {
                    "summary": "Get patient's latest values for all measured components",
                    "description": "This method returns the latest values of all the blood test components measured on a patient (identified by his SSN), to be used by medical doctors",
                    "operationId": "getLastValuesForBloodTest",
                    "parameters": [{
                        "name": "SSN",
                        "in": "path",
                        "description": "SSN of the patient",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }],
                    "responses": {
                        "200": {
                            "description": "Latest values of all blood test components measured on the patient",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "components": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "date": {
                                                            "type": "string"
                                                        },
                                                        "name": {
                                                            "type": "string"
                                                        },
                                                        "value": {
                                                            "type": "object"
                                                        }
                                                    },
                                                    "required": ["date", "name", "value"]
                                                }
                                            },
                                            "_links": {
                                                "type": "object",
                                                "properties": {
                                                    "self": {
                                                        "type": "object",
                                                        "properties": {
                                                            "href": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "required": ["href"]
                                                    },
                                                    "patient": {
                                                        "type": "object",
                                                        "properties": {
                                                            "href": {
                                                                "type": "string"
                                                            }
                                                        },
                                                        "required": ["href"]
                                                    },
                                                    "timeseries": {
                                                        "type": "object",
                                                        "properties": {
                                                            "templated": {
                                                                "type": "boolean",
                                                                "default": true
                                                            },
                                                            "href": {
                                                                "type": "string",
                                                                "default": "/patient/some-SSN/blood-test/component/{name}"
                                                            }
                                                        },
                                                        "required": ["href", "templated"]
                                                    }
                                                },
                                                "required": ["self", "patient", "timeseries"]
                                            }
                                        },
                                        "required": ["components", "_links"]
                                    }
                                }
                            }
                        },
                        "404": {
                            "description": "Patient not found"
                        }
                    },
                    "x-data_sources": ["bloodTestSource", "patientBiographicalData"]
                }
            },
