                    }, {
                        "name": "component",
                        "in": "path",
                        "description": "Blood test component for which the values are requested",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }],
                    "responses": {
                        "200": {
                            "description": "Values for the specified blood test component in the patient's blood tests",
                            "content": {
                                "application/json": {
                                    "schema": {
                                        "type": "object",
                                        "properties": {
                                            "series": {
                                                "type": "array",
                                                "items": {
                                                    "type": "object",
                                                    "properties": {
                                                        "date": {
                                                            "type": "string"
                                                        },
                                                        "value": {
                                                            "type": "object"
                                                        }
                                                    },
                                                    "required": ["date", "value"]
                                                }
                                            },
                                            "_links": {
                                                "type": "object",
                                                "properties": {
                                                    "self": {
                                                        "type": "object",
                                                        "properties": {
                                                            "href": {
                                                                "type": "string"
                                                            }
                                                        },
